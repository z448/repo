#!/usr/bin/env perl

use Mojolicious::Lite;
use open qw<:encoding(UTF-8)>;
use Repo::Packages qw< digest packages >;

plugin( 'Directory', root => "/Users/tmp/Documents/mojo-repository" )->start;

my $packages = packages("/Users/tmp/Documents/repo/deb");

for my $package (@$packages){
    for(@$package){
        print $_ . "\n";
    }
};
